<?xml version="1.0" encoding="utf-8" ?>
<Database Name="Diversity" xmlns="http://schemas.microsoft.com/linqtosql/mapping/2007">
  <Table Name="EventSeries" Member="DiversityService.Model.EventSeries">
    <Type Name="DiversityService.Model.EventSeries">
      <Column Name="SeriesID" Member="SeriesID" IsPrimaryKey="true" />
      <Column Name="Description" Member="Description" />
      <Column Name="SeriesCode" Member="SeriesCode"/>
      <Column Name="SeriesStart" Member="SeriesStart"/>
      <Column Name="SeriesEnd" Member="SeriesEnd"/>
      <Column Name="IsModified" Member="IsModified" CanBeNull="true" />
    </Type>
  </Table>
  <Table Name="Event" Member="DiversityService.Model.Event">
    <Type Name="DiversityService.Model.Event">
      <Column Name="EventID" Member="EventID" IsPrimaryKey="true"/>
      <Column Name="SeriesID" Member="SeriesID"/>
      <Column Name="CollectionDate" Member="CollectionDate"/>
      <Column Name="LocalityDescription" Member="LocalityDescription"/>
      <Column Name="HabitatDescription" Member="HabitatDescription"/>
      <Column Name="IsModified" Member="IsModified" CanBeNull="true"/>
    </Type>    
  </Table>
  <Table Name="Term" Member="DiversityService.Model.Term">
    <Type Name="DiversityService.Model.Term">
      <Column Name="Code" Member="Code" IsPrimaryKey="true" />
      <Column Name="SourceID" Member="SourceID" IsPrimaryKey="true" />
      <Column Name="Description" Member="Description" />
      <Column Name="DisplayText" Member="DisplayText" />
      <Column Name="ParentCode" Member="ParentCode" CanBeNull="true"/>
    </Type>
  </Table>
  <Table Name="IdentificationUnit" Member="DiversityService.Model.IdentificationUnit">
    <Type Name="DiversityService.Model.IdentificationUnit">
      <Column Name="UnitID" Member="UnitID" IsPrimaryKey="true"/>
      <Column Name="RelatedUnitID" Member="RelatedUnitID"/>
      <Column Name="AccessionNumber" Member="AccessionNumber"/>
      <Column Name="TaxonomicGroup" Member="TaxonomicGroup"/>
      <Column Name="RelationType" Member="RelationType"/>
      <Column Name="ColonisedSubstratePart" Member="ColonisedSubstratePart"/>
      <Column Name="LifeStage" Member="LifeStage"/>
      <Column Name="Gender" Member="Gender"/>
      <Column Name="UnitIdentifier" Member="UnitIdentifier"/>
      <Column Name="UnitDescription" Member="UnitDescription"/>
      <Column Name="IsModified" Member="IsModified" CanBeNull="true"/>      
    </Type>
  </Table>
  <Table Name="TaxonName" Member="DiversityService.Model.TaxonName">
    <Type Name="DiversityService.Model.TaxonName">
      <Column Name="URI" Member="URI" IsPrimaryKey="true"/>
      <Column Name="TaxonNameCache" Member="TaxonNameCache"/>
      <Column Name="TaxonNameSinAuth" Member="TaxonNameSinAuth"/>
      <Column Name="GenusOrSupragenic" Member="GenusOrSupragenic"/>
      <Column Name="SpeciesEpithet" Member="SpeciesEpithet"/>
      <Column Name="InfraspecificEpithet" Member="InfraspecificEpithet"/>
    </Type>
  </Table>
</Database>

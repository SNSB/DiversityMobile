<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="DiversityPhoneScreens.Field"
	Width="480" Height="800">

	<Grid x:Name="LayoutRoot" Background="White">
		<TextBlock Height="60" Margin="69,27,117,0" Style="{StaticResource BasicTextBlock-Sketch}" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="29.333" Text="DiversityPhone"/>
		<Button Content="Upload" HorizontalAlignment="Left" Height="46" Margin="8,0,0,8" Style="{StaticResource Button-Sketch}" VerticalAlignment="Bottom" Width="220" RenderTransformOrigin="-0.026,0.565"/>
		<Button Content="Download" HorizontalAlignment="Right" Height="46" Margin="0,0,8,8" Style="{StaticResource Button-Sketch}" VerticalAlignment="Bottom" Width="220"/>
		<ListBox Margin="8,91,8,79">
			<ListBoxItem Content="Recent IU" Margin="0" Style="{StaticResource ListBoxItem-Sketch}" Height="27"/>
			<ListBoxItem Content="Recent Event" Margin="0" Style="{StaticResource ListBoxItem-Sketch}" Height="27"/>
			<ListBoxItem Content="Recent Analysis" Margin="0" Style="{StaticResource ListBoxItem-Sketch}" Height="27"/>
			<ListBoxItem Content="..." Margin="0" Style="{StaticResource ListBoxItem-Sketch}" Height="27"/>
		</ListBox>
	</Grid>
</UserControl>